{
  "name": "nft-collection",
  "description": "nft-collection monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "web",
    "contract"
  ],
  "scripts": {
    "build": "yarn workspace @nft-collection/web build",
    "start": "yarn workspace @nft-collection/web start",
    "deploy": "yarn workspace @nft-collection/contract deploy",
    "deploy:rinkeby": "yarn workspace @nft-collection/contract deploy:rinkeby",
    "verify": "yarn workspace @nft-collection/contract verify"
  }
}
